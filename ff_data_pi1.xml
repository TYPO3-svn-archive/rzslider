<T3DataStructure>
<meta>
<langDisable>1</langDisable>
</meta>
  <sheets>
    <sDEF>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.sheet_general</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
       <ce>
          <TCEforms>
            <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.ce</label>
            <config>
              <type>group</type>
              <internal_type>db</internal_type>
              <allowed>tt_content</allowed>
              <show_thumbs>1</show_thumbs>
              <size>5</size>
              <autoSizeMax>5</autoSizeMax>
              <maxitems>100</maxitems>
              <minitems>0</minitems>
            </config>
          </TCEforms>
        </ce>
        <headlines>
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.headlines</label>
          <config>
            <type>text</type>
            <cols>30</cols>
            <rows>5</rows>
          </config>
          </TCEforms>
        </headlines> 	
        <onlytabheadlines>
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.onlytabheadlines</label>
          <config>
            <type>check</type>
            <checkbox></checkbox>
            <size>1</size>
            <default>1</default>
          </config>
          </TCEforms>
        </onlytabheadlines> 	 
        </el>
      </ROOT>
    </sDEF>
    <options>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.sheet_options</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
       <!--
       <template>
          <TCEforms>
            <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.template</label>
            <config>
              <type>input</type>
              <size>25</size>
              <default></default>
              <wizards type="array">
                <_PADDING>2</_PADDING>
                <link type="array">
                <type>popup</type>
                <title>Template</title>
                <icon>link_popup.gif</icon>
                <script>browse_links.php?mode=wizard&amp;act=file</script>
                <params type="array">
                  <blindLinkOptions>page,url,mail,spec</blindLinkOptions>
                  <allowedExtensions>htm,html,tmpl,tpl</allowedExtensions>
                </params>
                <JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
                </link>
              </wizards>
            </config>
          </TCEforms>
        </template>
        -->
        <position>
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.position</label>
          <config>
            <type>input</type>
            <size>5</size>
          </config>
          </TCEforms>
        </position>
        <autoslide>
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoslide</label>
          <config>
            <type>check</type>
            <checkbox></checkbox>
            <size>1</size>
          </config>
          <onChange>reload</onChange>
          </TCEforms>
        </autoslide> 
        <autoslideinterval>
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoslideinterval</label>
          <displayCond>FIELD:autoslide:=:1</displayCond>
          <config>
            <type>input</type>
            <size>5</size>
          </config>
          </TCEforms>
        </autoslideinterval>
        <autoslidestopwhenclicked>
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoslidestopwhenclicked</label>
          <displayCond>FIELD:autoslide:=:1</displayCond>
          <config>
            <type>check</type>
            <checkbox></checkbox>
            <size>1</size>
            <default>1</default>
          </config>
          </TCEforms>
        </autoslidestopwhenclicked> 
        <dynamictabs>
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.dynamictabs</label>
          <config>
            <type>check</type>
            <checkbox>1</checkbox>
            <size>1</size>
            <default>1</default>
          </config>
          <onChange>reload</onChange>
          </TCEforms>
        </dynamictabs>
        <dynamictabsposition>        
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.dynamictabsposition</label>
          <displayCond>FIELD:dynamictabs:=:1</displayCond>
          <config>
            <type>select</type>
            <items type="array">
            <numIndex index="0" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.dynamictabsposition_top</numIndex>
            <numIndex index="1">top</numIndex>
            </numIndex>       
            <numIndex index="1" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.dynamictabsposition_bottom</numIndex>
            <numIndex index="1">bottom</numIndex>   
            </numIndex>           
            </items>          
            <maxitems>1</maxitems>
            <size>1</size>          
          </config>
          </TCEforms>
        </dynamictabsposition>  
        <dynamictabsalign>        
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.dynamictabsalign</label>
          <displayCond>FIELD:dynamictabs:=:1</displayCond>
          <config>
            <type>select</type>
            <items type="array">
            <numIndex index="0" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.dynamictabsalign_center</numIndex>
            <numIndex index="1">center</numIndex>
            </numIndex>       
            <numIndex index="1" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.dynamictabsalign_left</numIndex>
            <numIndex index="1">left</numIndex>   
            </numIndex>       
            <numIndex index="2" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.dynamictabsalign_right</numIndex>
            <numIndex index="1">right</numIndex>   
            </numIndex>           
            </items>          
            <maxitems>1</maxitems>
            <size>1</size>          
          </config>
          </TCEforms>
        </dynamictabsalign>   
        <dynamicarrows>
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.dynamicarrows</label>
          <config>
            <type>check</type>
            <checkbox>1</checkbox>
            <size>1</size>
            <default>1</default>
          </config>
          </TCEforms>
        </dynamicarrows>
        <slideeasefunction>        
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.slideEaseFunction</label>
          <config>
            <type>select</type>
            <items type="array">
            <numIndex index="0" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.slideEaseFunction_1</numIndex>
            <numIndex index="1">easeInOutExpo</numIndex>
            </numIndex>       
            <numIndex index="1" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.slideEaseFunction_2</numIndex>
            <numIndex index="1">easeOutBounce</numIndex>   
            </numIndex>       
            <numIndex index="2" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.slideEaseFunction_3</numIndex>
            <numIndex index="1">easeOutCirc</numIndex>   
            </numIndex>    
            <numIndex index="3" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.slideEaseFunction_4</numIndex>
            <numIndex index="1">easeOutSine</numIndex>   
            </numIndex>  
            <numIndex index="4" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.slideEaseFunction_5</numIndex>
            <numIndex index="1">easeOutBack</numIndex>   
            </numIndex>  
            <numIndex index="5" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.slideEaseFunction_6</numIndex>
            <numIndex index="1">easeOutElastic</numIndex>   
            </numIndex>   
            <numIndex index="5" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.slideEaseFunction_7</numIndex>
            <numIndex index="1">easeOutQuint</numIndex>   
            </numIndex>                                                          
            </items>          
            <maxitems>1</maxitems>
            <size>1</size>          
          </config>
          </TCEforms>
        </slideeasefunction>
        <slideeaseduration>
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.slideeaseduration</label>
          <config>
            <type>input</type>
            <size>5</size>
          </config>
          </TCEforms>
        </slideeaseduration> 
        <autoheight>
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoheight</label>
          <config>
            <type>check</type>
            <checkbox>1</checkbox>
            <size>1</size>
            <default>1</default>
          </config>
          <onChange>reload</onChange>
          </TCEforms>
        </autoheight>          
        <autoheighteasefunction>        
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoHeightEaseFunction</label>
          <displayCond>FIELD:autoheight:=:1</displayCond>
          <config>
            <type>select</type>
            <items type="array">
            <numIndex index="0" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoHeightEaseFunction_1</numIndex>
            <numIndex index="1">easeInOutExpo</numIndex>
            </numIndex>       
            <numIndex index="1" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoHeightEaseFunction_2</numIndex>
            <numIndex index="1">easeOutBounce</numIndex>   
            </numIndex>       
            <numIndex index="2" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoHeightEaseFunction_3</numIndex>
            <numIndex index="1">easeOutCirc</numIndex>   
            </numIndex>    
            <numIndex index="3" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoHeightEaseFunction_4</numIndex>
            <numIndex index="1">easeOutSine</numIndex>   
            </numIndex>  
            <numIndex index="4" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoHeightEaseFunction_5</numIndex>
            <numIndex index="1">easeOutBack</numIndex>   
            </numIndex>  
            <numIndex index="5" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoHeightEaseFunction_6</numIndex>
            <numIndex index="1">easeOutElastic</numIndex>   
            </numIndex>   
            <numIndex index="5" type="array">
            <numIndex index="0">LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoHeightEaseFunction_7</numIndex>
            <numIndex index="1">easeOutQuint</numIndex>   
            </numIndex>                                                          
            </items>          
            <maxitems>1</maxitems>
            <size>1</size>          
          </config>
          </TCEforms>
        </autoheighteasefunction>
        <autoheighteaseduration>
          <TCEforms>
          <label>LLL:EXT:rzslider/locallang_tca.xml:rzslider.pi_flexform.autoheighteaseduration</label>
          <displayCond>FIELD:autoheight:=:1</displayCond>
          <config>
            <type>input</type>
            <size>5</size>
          </config>
          </TCEforms>
        </autoheighteaseduration>                   
        </el>
      </ROOT>
    </options>
  </sheets>
</T3DataStructure>